{
    "run_name": "PCam_NScene",
    "run_dir": "./output", 
    "wsi_dir": "./data/wsi",
    "tile_dir": "./data/tiles",
    "class_names": "normal,tumor", 
    "data_loader": {
        "dataset_type": "TiledDataset",
        "args":{
            "data_dir": "./data/tiles",
            "train_subd": "train", 
            "val_subd": "val", 
            "test_subd": "test", 
            "batch_size": 128,
            "shuffle": true,
            "num_workers": 1
        }
    },
    "model": {
        "arch": "NaturalSceneClassification",
        "args": {}
    },
    "loss": "CrossEntropyLoss",
    "trainer": {
        "epochs": 50,
        "metrics": ["accuracy", "loss"]
    },
    "optimizer": {
        "type": "Adam",
        "args":{
            "lr": 0.0001
        }
    },
    "tester": {
        "metrics": ["accuracy", "precision", "recall", "f1", "auc"]
    },
    "output": {
        "image_sample": "image_sample.jpg",
        "config_outfile": "final_config.json",
        "model_summary_file": "model.log",
        "final_model": "final_model.pt",
        "running_model": "running_model.pt",
        "train_logfile": "training.log",
        "stats_file": "stats.csv",
        "loss_plot": "plot_loss.png",
        "acc_plot": "plot_acc.png",
        "test_log": "test.log",
        "metrics_file": "test_metrics.csv",
        "confusion_plot": "plot_confusion.png"
    },
    "notes": {}
}